{
  "PropertySetDef": {
    "xmlns:xsd": "http://www.w3.org/2001/XMLSchema",
    "xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance",
    "ifdguid": "9f80f380d21511e1800000215ad4efdf",
    "templatetype": "PSET_TYPEDRIVENOVERRIDE",
    "xsi:noNamespaceSchemaLocation": "http://buildingSMART-tech.org/xml/psd/PSD_IFC4.xsd",
    "IfcVersion": {
      "version": "IFC4"
    },
    "Name": "Pset_WallCommon",
    "Definition": "Properties common to the definition of all occurrences of IfcWall and IfcWallStandardCase.",
    "Applicability": null,
    "ApplicableClasses": {
      "ClassName": "IfcWall"
    },
    "ApplicableTypeValue": "IfcWall",
    "PropertyDefs": {
      "PropertyDef": [
        {
          "ifdguid": "aa3b8880d21511e1800000215ad4efdf",
          "Name": "Reference",
          "Definition": "Reference ID for this specified type in this project (e.g. type 'A-1'), Also referred to as \"construction type\". It should be provided as an alternative to the name of the \"object type\", if the software does not support object types.",
          "PropertyType": {
            "TypePropertySingleValue": {
              "DataType": {
                "type": "IfcIdentifier"
              }
            }
          }
        },
        {
          "ifdguid": "b0ca0000d21511e1800000215ad4efdf",
          "Name": "Status",
          "Definition": "Status of the element, predominately used in renovation or retrofitting projects. The status can be assigned to as \"New\" - element designed as new addition, \"Existing\" - element exists and remains, \"Demolish\" - element existed but is to be demolished,  \"Temporary\" - element will exists only temporary (like a temporary support structure).",
          "PropertyType": {
            "TypePropertyEnumeratedValue": {
              "EnumList": {
                "name": "PEnum_ElementStatus",
                "EnumItem": [
                  "NEW",
                  "EXISTING",
                  "DEMOLISH",
                  "TEMPORARY",
                  "OTHER",
                  "NOTKNOWN",
                  "UNSET"
                ]
              },
              "ConstantList": {
                "ConstantDef": [
                  {
                    "Name": "NEW",
                    "Definition": "element designed as new addition",
                    "NameAliases": {
                      "NameAlias": {
                        "lang": "en",
                        "#text": "New"
                      }
                    },
                    "DefinitionAliases": {
                      "DefinitionAlias": {
                        "lang": "en"
                      }
                    }
                  },
                  {
                    "Name": "EXISTING",
                    "Definition": "element exists and is to remain",
                    "NameAliases": {
                      "NameAlias": {
                        "lang": "en",
                        "#text": "Existing"
                      }
                    },
                    "DefinitionAliases": {
                      "DefinitionAlias": {
                        "lang": "en"
                      }
                    }
                  },
                  {
                    "Name": "DEMOLISH",
                    "Definition": "element exists but is to be demolished",
                    "NameAliases": {
                      "NameAlias": {
                        "lang": "en",
                        "#text": "Demolish"
                      }
                    },
                    "DefinitionAliases": {
                      "DefinitionAlias": {
                        "lang": "en"
                      }
                    }
                  },
                  {
                    "Name": "TEMPORARY",
                    "Definition": "element will exist only temporarily (such as a temporary support structure)",
                    "NameAliases": {
                      "NameAlias": {
                        "lang": "en",
                        "#text": "Temporary"
                      }
                    },
                    "DefinitionAliases": {
                      "DefinitionAlias": {
                        "lang": "en"
                      }
                    }
                  },
                  {
                    "Name": "OTHER",
                    "NameAliases": {
                      "NameAlias": {
                        "lang": "en",
                        "#text": "(other)"
                      }
                    },
                    "DefinitionAliases": {
                      "DefinitionAlias": {
                        "lang": "en",
                        "#text": "Value is not listed."
                      }
                    }
                  },
                  {
                    "Name": "NOTKNOWN",
                    "NameAliases": {
                      "NameAlias": {
                        "lang": "en",
                        "#text": "(unknown)"
                      }
                    },
                    "DefinitionAliases": {
                      "DefinitionAlias": {
                        "lang": "en",
                        "#text": "Value is unknown."
                      }
                    }
                  },
                  {
                    "Name": "UNSET",
                    "NameAliases": {
                      "NameAlias": {
                        "lang": "en",
                        "#text": "(unset)"
                      }
                    },
                    "DefinitionAliases": {
                      "DefinitionAlias": {
                        "lang": "en",
                        "#text": "Value has not been specified."
                      }
                    }
                  }
                ]
              }
            }
          }
        },
        {
          "ifdguid": "c17a7600d21511e1800000215ad4efdf",
          "Name": "AcousticRating",
          "Definition": "Acoustic rating for this object.\nIt is provided according to the national building code. It indicates the sound transmission resistance of this object by an index ratio (instead of providing full sound absorbtion values).",
          "PropertyType": {
            "TypePropertySingleValue": {
              "DataType": {
                "type": "IfcLabel"
              }
            }
          }
        },
        {
          "ifdguid": "c93a1a80d21511e1800000215ad4efdf",
          "Name": "FireRating",
          "Definition": "Fire rating given according to the national fire safety classification.",
          "PropertyType": {
            "TypePropertySingleValue": {
              "DataType": {
                "type": "IfcLabel"
              }
            }
          }
        },
        {
          "ifdguid": "cfc89200d21511e1800000215ad4efdf",
          "Name": "Combustible",
          "Definition": "Indication whether the object is made from combustible material (TRUE) or not (FALSE).",
          "PropertyType": {
            "TypePropertySingleValue": {
              "DataType": {
                "type": "IfcBoolean"
              }
            }
          }
        },
        {
          "ifdguid": "d6570980d21511e1800000215ad4efdf",
          "Name": "SurfaceSpreadOfFlame",
          "Definition": "Indication on how the flames spread around the surface,\nIt is given according to the national building code that governs the fire behaviour for materials.",
          "PropertyType": {
            "TypePropertySingleValue": {
              "DataType": {
                "type": "IfcLabel"
              }
            }
          }
        },
        {
          "ifdguid": "de16ae00d21511e1800000215ad4efdf",
          "Name": "ThermalTransmittance",
          "Definition": "Thermal transmittance coefficient (U-Value) of a material.\nHere the total thermal transmittance coefficient through the wall (including all materials).",
          "PropertyType": {
            "TypePropertySingleValue": {
              "DataType": {
                "type": "IfcThermalTransmittanceMeasure"
              }
            }
          }
        },
        {
          "ifdguid": "e2db6200d21511e1800000215ad4efdf",
          "Name": "IsExternal",
          "Definition": "Indication whether the element is designed for use in the exterior (TRUE) or not (FALSE). If (TRUE) it is an external element and faces the outside of the building.",
          "PropertyType": {
            "TypePropertySingleValue": {
              "DataType": {
                "type": "IfcBoolean"
              }
            }
          }
        },
        {
          "ifdguid": "f1c21480d21511e1800000215ad4efdf",
          "Name": "LoadBearing",
          "Definition": "Indicates whether the object is intended to carry loads (TRUE) or not (FALSE).",
          "PropertyType": {
            "TypePropertySingleValue": {
              "DataType": {
                "type": "IfcBoolean"
              }
            }
          }
        },
        {
          "ifdguid": "ea027000d21511e1800000215ad4efdf",
          "Name": "ExtendToStructure",
          "Definition": "Indicates whether the object extend to the structure above (TRUE) or not (FALSE).",
          "PropertyType": {
            "TypePropertySingleValue": {
              "DataType": {
                "type": "IfcBoolean"
              }
            }
          }
        },
        {
          "ifdguid": "f981b900d21511e1800000215ad4efdf",
          "Name": "Compartmentation",
          "Definition": "Indication whether the object is designed to serve as a fire compartmentation (TRUE) or not (FALSE).",
          "PropertyType": {
            "TypePropertySingleValue": {
              "DataType": {
                "type": "IfcBoolean"
              }
            }
          }
        }
      ]
    }
  }
}