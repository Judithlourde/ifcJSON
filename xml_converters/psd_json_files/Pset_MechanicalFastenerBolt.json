    "Name": "Pset_MechanicalFastenerBolt",
    "Definition": "Properties related to bolt-type fasteners. The properties of a whole set with bolt, washers and nut may be provided. Note, it is usually not necessary to transmit these properties in case of standardized bolts. Instead, the standard is referred to.",
    "Applicability": null,
    "ApplicableClasses": {
      "ClassName": "IfcMechanicalFastener/BOLT"
    },
    "ApplicableTypeValue": "IfcMechanicalFastener/BOLT",
    "PropertyDefs": {
      "PropertyDef": [
        {
          "ifdguid": "e7d9ea80d1f011e1800000215ad4efdf",
          "Name": "ThreadDiameter",
          "Definition": "Nominal diameter of the thread, if different from the bolt's overall nominal diameter",
          "PropertyType": {
            "TypePropertySingleValue": {
              "DataType": {
                "type": "IfcPositiveLengthMeasure"
              }
            }
          }
        },
        {
          "ifdguid": "ee686200d1f011e1800000215ad4efdf",
          "Name": "ThreadLength",
          "Definition": "Nominal length of the thread",
          "PropertyType": {
            "TypePropertySingleValue": {
              "DataType": {
                "type": "IfcPositiveLengthMeasure"
              }
            }
          }
        },
        {
          "ifdguid": "f45e4300d1f011e1800000215ad4efdf",
          "Name": "NutsCount",
          "Definition": "Count of nuts to be mounted on one bolt",
          "PropertyType": {
            "TypePropertySingleValue": {
              "DataType": {
                "type": "IfcCountMeasure"
              }
            }
          }
        },
        {
          "ifdguid": "fa542400d1f011e1800000215ad4efdf",
          "Name": "WashersCount",
          "Definition": "Count of washers to be mounted on one bolt",
          "PropertyType": {
            "TypePropertySingleValue": {
              "DataType": {
                "type": "IfcCountMeasure"
              }
            }
          }
        },
        {
          "ifdguid": "ff18d800d1f011e1800000215ad4efdf",
          "Name": "HeadShape",
          "Definition": "Shape of the bolt's head, e.g. 'Hexagon', 'Countersunk', 'Cheese'",
          "PropertyType": {
            "TypePropertySingleValue": {
              "DataType": {
                "type": "IfcLabel"
              }
            }
          }
        },
        {
          "ifdguid": "05a74f80d1f111e1800000215ad4efdf",
          "Name": "KeyShape",
          "Definition": "If applicable, shape of the head's slot, e.g. 'Slot', 'Allen'",
          "PropertyType": {
            "TypePropertySingleValue": {
              "DataType": {
                "type": "IfcLabel"
              }
            }
          }
        },
        {
          "ifdguid": "0b9d3080d1f111e1800000215ad4efdf",
          "Name": "NutShape",
          "Definition": "Shape of the nut, e.g. 'Hexagon', 'Cap', 'Castle', 'Wing'",
          "PropertyType": {
            "TypePropertySingleValue": {
              "DataType": {
                "type": "IfcLabel"
              }
            }
          }
        },
        {
          "ifdguid": "1d7ed380d1f111e1800000215ad4efdf",
          "Name": "WasherShape",
          "Definition": "Shape of the washers, e.g. 'Standard', 'Square'",
          "PropertyType": {
            "TypePropertySingleValue": {
              "DataType": {
                "type": "IfcLabel"
              }
            }
          }
        }
      ]
    }
