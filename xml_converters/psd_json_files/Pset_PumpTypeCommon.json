          "PropertyType": {
            "TypePropertyEnumeratedValue": {
              "EnumList": {
                "name": "PEnum_ElementStatus",
                "EnumItem": [
                  "NEW",
                  "EXISTING",
                  "DEMOLISH",
                  "TEMPORARY",
                  "OTHER",
                  "NOTKNOWN",
                  "UNSET"
                ]
              },
              "ConstantList": {
                "ConstantDef": [
                  {
                    "Name": "NEW",
                    "Definition": "element designed as new addition",
                    "NameAliases": {
