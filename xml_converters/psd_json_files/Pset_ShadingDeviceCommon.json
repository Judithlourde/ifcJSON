    "Name": "Pset_ShadingDeviceCommon",
    "Definition": "Shading device properties associated with an element that represents a shading device",
    "Applicability": null,
    "ApplicableClasses": {
      "ClassName": "IfcShadingDevice"
    },
    "ApplicableTypeValue": "IfcShadingDevice",
    "PropertyDefs": {
      "PropertyDef": [
        {
          "ifdguid": "47db8b80d20711e1800000215ad4efdf",
          "Name": "Reference",
          "Definition": "Reference ID for this specified type in this project (e.g. type 'A-1'), provided, if there is no classification reference to a recognized classification system used.",
          "PropertyType": {
            "TypePropertySingleValue": {
              "DataType": {
                "type": "IfcIdentifier"
              }
            }
          }
        },
        {
          "ifdguid": "4e6a0300d20711e1800000215ad4efdf",
          "Name": "Status",
          "Definition": "Status of the element, predominately used in renovation or retrofitting projects. The status can be assigned to as \"New\" - element designed as new addition, \"Existing\" - element exists and remains, \"Demolish\" - element existed but is to be demolished,  \"Temporary\" - element will exists only temporary (like a temporary support structure).",
          "PropertyType": {
            "TypePropertyEnumeratedValue": {
              "EnumList": {
                "name": "PEnum_ElementStatus",
                "EnumItem": [
                  "NEW",
                  "EXISTING",
                  "DEMOLISH",
                  "TEMPORARY",
                  "OTHER",
                  "NOTKNOWN",
                  "UNSET"
                ]
              },
              "ConstantList": {
                "ConstantDef": [
                  {
                    "Name": "NEW",
                    "Definition": "element designed as new addition",
                    "NameAliases": {
                      "NameAlias": {
                        "lang": "en",
                        "#text": "New"
                      }
                    },
                    "DefinitionAliases": {
                      "DefinitionAlias": {
                        "lang": "en"
                      }
                    }
                  },
                  {
                    "Name": "EXISTING",
                    "Definition": "element exists and is to remain",
                    "NameAliases": {
                      "NameAlias": {
                        "lang": "en",
                        "#text": "Existing"
                      }
                    },
                    "DefinitionAliases": {
                      "DefinitionAlias": {
                        "lang": "en"
                      }
                    }
                  },
                  {
                    "Name": "DEMOLISH",
                    "Definition": "element exists but is to be demolished",
                    "NameAliases": {
                      "NameAlias": {
                        "lang": "en",
                        "#text": "Demolish"
                      }
                    },
                    "DefinitionAliases": {
                      "DefinitionAlias": {
                        "lang": "en"
                      }
                    }
                  },
                  {
                    "Name": "TEMPORARY",
                    "Definition": "element will exist only temporarily (such as a temporary support structure)",
                    "NameAliases": {
                      "NameAlias": {
                        "lang": "en",
                        "#text": "Temporary"
                      }
                    },
                    "DefinitionAliases": {
                      "DefinitionAlias": {
                        "lang": "en"
                      }
                    }
                  },
                  {
                    "Name": "OTHER",
                    "NameAliases": {
                      "NameAlias": {
                        "lang": "en",
                        "#text": "(other)"
                      }
                    },
                    "DefinitionAliases": {
                      "DefinitionAlias": {
                        "lang": "en",
                        "#text": "Value is not listed."
                      }
                    }
                  },
                  {
                    "Name": "NOTKNOWN",
                    "NameAliases": {
                      "NameAlias": {
                        "lang": "en",
                        "#text": "(unknown)"
                      }
                    },
                    "DefinitionAliases": {
                      "DefinitionAlias": {
                        "lang": "en",
                        "#text": "Value is unknown."
                      }
                    }
                  },
                  {
                    "Name": "UNSET",
                    "NameAliases": {
                      "NameAlias": {
                        "lang": "en",
                        "#text": "(unset)"
                      }
                    },
                    "DefinitionAliases": {
                      "DefinitionAlias": {
                        "lang": "en",
                        "#text": "Value has not been specified."
                      }
                    }
                  }
                ]
              }
            }
          }
        },
        {
          "ifdguid": "5de94c00d20711e1800000215ad4efdf",
          "Name": "ShadingDeviceType",
          "Definition": "Specifies the type of shading device.",
          "PropertyType": {
            "TypePropertyEnumeratedValue": {
              "EnumList": {
                "name": "PEnum_ElementShading",
                "EnumItem": [
                  "FIXED",
                  "MOVABLE",
                  "OVERHANG",
                  "SIDEFIN",
                  "USERDEFINED",
                  "NOTDEFINED"
                ]
              },
              "ConstantList": {
                "ConstantDef": [
                  {
                    "Name": "FIXED",
                    "NameAliases": {
                      "NameAlias": {
                        "lang": "en",
                        "#text": "Fixed"
                      }
                    },
                    "DefinitionAliases": {
                      "DefinitionAlias": {
                        "lang": "en"
                      }
                    }
                  },
                  {
                    "Name": "MOVABLE",
                    "NameAliases": {
                      "NameAlias": {
                        "lang": "en",
