{
  "PropertySetDef": {
    "xmlns:xsd": "http://www.w3.org/2001/XMLSchema",
    "xmlns:xsi": "http://www.w3.org/2001/XMLSchema-instance",
    "ifdguid": "11364e80d1d011e1800000215ad4efdf",
    "templatetype": "PSET_TYPEDRIVENOVERRIDE",
    "xsi:noNamespaceSchemaLocation": "http://buildingSMART-tech.org/xml/psd/PSD_IFC4.xsd",
    "IfcVersion": {
      "version": "IFC4"
    },
    "Name": "Pset_CoveringCommon",
    "Definition": "Properties common to the definition of all occurrence and type objects of covering",
    "Applicability": null,
    "ApplicableClasses": {
      "ClassName": "IfcCovering"
    },
    "ApplicableTypeValue": "IfcCovering",
    "PropertyDefs": {
      "PropertyDef": [
        {
          "ifdguid": "14c9d580d1d011e1800000215ad4efdf",
          "Name": "Reference",
          "Definition": "Reference ID for this specified type in this project (e.g. type 'A-1'), Also referred to as \"construction type\". It should be provided as an alternative to the name of the \"object type\", if the software does not support object types.",
          "PropertyType": {
            "TypePropertySingleValue": {
              "DataType": {
                "type": "IfcIdentifier"
              }
            }
          }
        },
        {
          "ifdguid": "1a272000d1d011e1800000215ad4efdf",
          "Name": "Status",
          "Definition": "Status of the element, predominately used in renovation or retrofitting projects. The status can be assigned to as \"New\" - element designed as new addition, \"Existing\" - element exists and remains, \"Demolish\" - element existed but is to be demolished,  \"Temporary\" - element will exists only temporary (like a temporary support structure).",
          "PropertyType": {
            "TypePropertyEnumeratedValue": {
              "EnumList": {
                "name": "PEnum_ElementStatus",
                "EnumItem": [
                  "NEW",
                  "EXISTING",
                  "DEMOLISH",
                  "TEMPORARY",
                  "OTHER",
                  "NOTKNOWN",
                  "UNSET"
                ]
              },
              "ConstantList": {
                "ConstantDef": [
                  {
                    "Name": "NEW",
                    "Definition": "element designed as new addition",
                    "NameAliases": {
                      "NameAlias": {
                        "lang": "en",
                        "#text": "New"
                      }
                    },
                    "DefinitionAliases": {
                      "DefinitionAlias": {
                        "lang": "en"
                      }
                    }
                  },
                  {
                    "Name": "EXISTING",
                    "Definition": "element exists and is to remain",
                    "NameAliases": {
                      "NameAlias": {
                        "lang": "en",
                        "#text": "Existing"
                      }
                    },
                    "DefinitionAliases": {
                      "DefinitionAlias": {
                        "lang": "en"
                      }
                    }
                  },
                  {
                    "Name": "DEMOLISH",
                    "Definition": "element exists but is to be demolished",
                    "NameAliases": {
                      "NameAlias": {
                        "lang": "en",
                        "#text": "Demolish"
                      }
                    },
                    "DefinitionAliases": {
                      "DefinitionAlias": {
                        "lang": "en"
                      }
                    }
                  },
                  {
                    "Name": "TEMPORARY",
                    "Definition": "element will exist only temporarily (such as a temporary support structure)",
                    "NameAliases": {
                      "NameAlias": {
                        "lang": "en",
                        "#text": "Temporary"
                      }
                    },
                    "DefinitionAliases": {
                      "DefinitionAlias": {
                        "lang": "en"
                      }
                    }
                  },
                  {
                    "Name": "OTHER",
                    "NameAliases": {
                      "NameAlias": {
                        "lang": "en",
                        "#text": "(other)"
                      }
                    },
                    "DefinitionAliases": {
                      "DefinitionAlias": {
                        "lang": "en",
                        "#text": "Value is not listed."
                      }
                    }
                  },
                  {
                    "Name": "NOTKNOWN",
                    "NameAliases": {
                      "NameAlias": {
                        "lang": "en",
                        "#text": "(unknown)"
                      }
                    },
                    "DefinitionAliases": {
                      "DefinitionAlias": {
                        "lang": "en",
                        "#text": "Value is unknown."
                      }
                    }
                  },
                  {
                    "Name": "UNSET",
                    "NameAliases": {
                      "NameAlias": {
                        "lang": "en",
                        "#text": "(unset)"
                      }
                    },
                    "DefinitionAliases": {
                      "DefinitionAlias": {
                        "lang": "en",
                        "#text": "Value has not been specified."
                      }
                    }
                  }
                ]
              }
            }
          }
        },
        {
          "ifdguid": "2e6b1d00d1d011e1800000215ad4efdf",
          "Name": "AcousticRating",
          "Definition": "Acoustic rating for this object.\nIt is giving according to the national building code. It indicates the sound transmission resistance of this object by an index ration (instead of providing full sound absorbtion values).",
          "PropertyType": {
            "TypePropertySingleValue": {
              "DataType": {
                "type": "IfcLabel"
              }
            }
          }
        },
        {
          "ifdguid": "3460fe00d1d011e1800000215ad4efdf",
          "Name": "FlammabilityRating",
          "Definition": "Flammability Rating for this object.\nIt is given according to the national building code that governs the rating of flammability for materials.",
          "PropertyType": {
            "TypePropertySingleValue": {
              "DataType": {
                "type": "IfcLabel"
              }
            }
          }
        },
        {
          "ifdguid": "39be4880d1d011e1800000215ad4efdf",
          "Name": "FragilityRating",
          "Definition": "Indication on the fragility of the covering (e.g., under fire conditions).  It is given according to the national building code that might provide a classification for fragility.",
          "PropertyType": {
            "TypePropertySingleValue": {
              "DataType": {
                "type": "IfcLabel"
              }
            }
          }
        },
        {
          "ifdguid": "45aa0a80d1d011e1800000215ad4efdf",
          "Name": "Combustible",
          "Definition": "Indication whether the object is made from combustible material (TRUE) or not (FALSE).",
          "PropertyType": {
            "TypePropertySingleValue": {
              "DataType": {
                "type": "IfcBoolean"
              }
            }
          }
        },
        {
          "ifdguid": "3f1b9300d1d011e1800000215ad4efdf",
          "Name": "SurfaceSpreadOfFlame",
          "Definition": "Indication on how the flames spread around the surface,\nIt is given according to the national building code that governs the fire behaviour for materials.",
          "PropertyType": {
            "TypePropertySingleValue": {
              "DataType": {
                "type": "IfcLabel"
              }
            }
          }
        },
        {
          "ifdguid": "565a8080d1d011e1800000215ad4efdf",
          "Name": "Finish",
          "Definition": "Finish selection for this object.\nHere specification of the surface finish for informational purposes.",
          "PropertyType": {
            "TypePropertySingleValue": {
              "DataType": {
                "type": "IfcText"
              }
            }
          }
        },
        {
          "ifdguid": "4b9feb80d1d011e1800000215ad4efdf",
          "Name": "IsExternal",
          "Definition": "Indication whether the element is designed for use in the exterior (TRUE) or not (FALSE). If (TRUE) it is an external element and faces the outside of the building.",
          "PropertyType": {
            "TypePropertySingleValue": {
              "DataType": {
                "type": "IfcBoolean"
              }
            }
          }
        },
        {
          "ifdguid": "50fd3600d1d011e1800000215ad4efdf",
          "Name": "ThermalTransmittance",
          "Definition": "Thermal transmittance coefficient (U-Value) of an element. \nHere the total thermal transmittance coefficient through the covering (including all materials).",
          "PropertyType": {
            "TypePropertySingleValue": {
              "DataType": {
                "type": "IfcThermalTransmittanceMeasure"
              }
            }
          }
        },
        {
          "ifdguid": "28753c00d1d011e1800000215ad4efdf",
          "Name": "FireRating",
          "Definition": "Fire rating for this object.\nIt is given according to the national fire safety classification.",
          "PropertyType": {
            "TypePropertySingleValue": {
              "DataType": {
                "type": "IfcLabel"
              }
            }
          }
        }
      ]
    }
  }
}