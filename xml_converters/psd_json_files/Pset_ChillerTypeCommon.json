        },
        {
          "ifdguid": "fe6c4b80d1c811e1800000215ad4efdf",
          "Name": "NominalCapacity",
          "Definition": "Nominal cooling capacity of chiller at standardized conditions as defined by the agency having jurisdiction.",
          "PropertyType": {
            "TypePropertySingleValue": {
              "DataType": {
                "type": "IfcPowerMeasure"
              }
            }
          }
        },
        {
          "ifdguid": "04622c80d1c911e1800000215ad4efdf",
          "Name": "NominalEfficiency",
          "Definition": "Nominal chiller efficiency under nominal conditions.",
          "PropertyType": {
            "TypePropertySingleValue": {
              "DataType": {
                "type": "IfcPositiveRatioMeasure"
              }
            }
          }
        },
        {
          "ifdguid": "0926e080d1c911e1800000215ad4efdf",
          "Name": "NominalCondensingTemperature",
          "Definition": "Chiller condensing temperature.",
          "PropertyType": {
            "TypePropertySingleValue": {
              "DataType": {
                "type": "IfcThermodynamicTemperatureMeasure"
              }
            }
          }
        },
        {
          "ifdguid": "0e842b00d1c911e1800000215ad4efdf",
          "Name": "NominalEvaporatingTemperature",
          "Definition": "Chiller evaporating temperature.",
          "PropertyType": {
            "TypePropertySingleValue": {
              "DataType": {
                "type": "IfcThermodynamicTemperatureMeasure"
              }
            }
          }
        },
        {
          "ifdguid": "147a0c00d1c911e1800000215ad4efdf",
          "Name": "NominalHeatRejectionRate",
          "Definition": "Sum of the refrigeration effect and the heat equivalent of the power input to the compressor.",
          "PropertyType": {
            "TypePropertySingleValue": {
              "DataType": {
                "type": "IfcPowerMeasure"
              }
            }
          }
        },
        {
          "ifdguid": "19d75680d1c911e1800000215ad4efdf",
          "Name": "NominalPowerConsumption",
          "Definition": "Nominal total power consumption.",
          "PropertyType": {
            "TypePropertySingleValue": {
              "DataType": {
                "type": "IfcPowerMeasure"
              }
            }
          }
        },
        {
          "ifdguid": "1e9c0a80d1c911e1800000215ad4efdf",
          "Name": "CapacityCurve",
          "Definition": "Chiller cooling capacity is a function of condensing temperature and evaporating temperature, data is in table form, Capacity = f (TempCon, TempEvp), capacity = a1+b1*Tei+c1*Tei^2+d1*Tci+e1*Tci^2+f1*Tei*Tci. \nThis table uses multiple input variables; to represent, both DefiningValues and DefinedValues lists are null and IfcTable is attached using IfcPropertyConstraintRelationship and IfcMetric.  Columns are specified in the following order: \n1.IfcPowerMeasure:Capacity\n2.IfcThermodynamicTemperatureMeasure:CondensingTemperature\n3.IfcThermodynamicTemperatureMeasure:EvaporatingTemperature",
          "PropertyType": {
            "TypePropertyTableValue": {
              "Expression": null,
              "DefiningValue": {
                "DataType": {
                  "type": "IfcThermodynamicTemperatureMeasure"
                }
              },
              "DefinedValue": {
                "DataType": {
                  "type": "IfcPowerMeasure"
                }
              }
            }
          }
        },
        {
          "ifdguid": "278cdc00d1c911e1800000215ad4efdf",
          "Name": "CoefficientOfPerformanceCurve",
          "Definition": "Chiller coefficient of performance (COP) is function of condensing temperature and evaporating temperature, data is in table form, COP= f (TempCon, TempEvp), COP = a2+b2*Tei+c2*Tei^2+d2*Tci+e2*Tci^2+f2*Tei*Tci. \nThis table uses multiple input variables; to represent, both DefiningValues and DefinedValues lists are null and IfcTable is attached using IfcPropertyConstraintRelationship and IfcMetric.  Columns are specified in the following order: \n1.IfcPositiveRatioMeasure:CoefficientOfPerformance\n2.IfcThermodynamicTemperatureMeasure:CondensingTemperature\n3.IfcThermodynamicTemperatureMeasure:EvaporatingTemperature",
          "PropertyType": {
            "TypePropertyTableValue": {
              "Expression": null,
              "DefiningValue": {
                "DataType": {
                  "type": "IfcThermodynamicTemperatureMeasure"
                }
              },
              "DefinedValue": {
                "DataType": {
                  "type": "IfcReal"
                }
              }
            }
          }
        },
        {
          "ifdguid": "2fe51700d1c911e1800000215ad4efdf",
          "Name": "FullLoadRatioCurve",
          "Definition": "Ratio of actual power to full load power as a quadratic function of part load, at certain condensing and evaporating temperature, FracFullLoadPower = f ( PartLoadRatio).",
          "PropertyType": {
            "TypePropertyTableValue": {
              "Expression": null,
              "DefiningValue": {
                "DataType": {
                  "type": "IfcPositiveRatioMeasure"
                }
              },
              "DefinedValue": {
                "DataType": {
                  "type": "IfcNormalisedRatioMeasure"
                }
              }
            }
          }
        }
      ]
